{
  "name": "@tobinbc/aws-utils",
  "version": "0.0.1",
  "description": "Various utility functions to complement the javascript AWS-SDK. Useful as a Lambda Layer.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "lambda": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tobinbc/aws-utils.git"
  },
  "keywords": [
    "aws",
    "sdk",
    "util",
    "chunk",
    "sqs",
    "lambda",
    "layer"
  ],
  "author": "Tobin Chew",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tobinbc/aws-utils/issues"
  },
  "homepage": "https://github.com/tobinbc/aws-utils#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3"
  },
  "files": [
    "dist/**/*",
    "*.json"
  ],
  "dependencies": {
    "@tobinbc/chunker": "^1.1.1",
    "aws-sdk": "^2.814.0"
  }
}
